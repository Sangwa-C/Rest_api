{{#partial "content"}}

    {{#if users}}
        {{#each users}}

            <div class="well" style="text-align:center">
                <small style=" text-align:center"> ~For every minute spent organizing, an hour is earned~  </small><br><br>

                <p> User Name: {{ name }} </p>
                <p> User Title {{ title }} </p>
                <p> User's Duty: {{ duty }} </p>
                <p> User's Department: {{ dname }} </p>
                
            </div>

        {{else}}

            <h1>blah blah</h1>


        {{/each}}
    {{/if}}


{{/partial}}

{{> layout.hbs}}